<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.mapper.GoodKcMapper">


    <select id="getSx" resultType="mallattr">
        select id,shxm_mch from t_mall_attr where flbh2=#{id}
    </select>

    <select id="getValue" resultType="mallValue">
        select id,shxzh from t_mall_value where shxm_id=#{id}
    </select>
    
    <insert id="addSku">
        insert into t_mall_sku(shp_id,kc,jg,sku_mch,chjshj) value(#{shp_id},#{kc},#{jg},#{sku_mch},new Date())
    </insert>
</mapper>